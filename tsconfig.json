{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "allowJs": true,
    "checkJs": false,
    "jsx": "react",
    "sourceMap": true,
    "removeComments": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "outDir": "./dist",
    "preserveConstEnums": true
  },
  "exclude": [
    "dist"
  ],
  "include": [
    "auth",
    "index.ts"
  ]
}
